  "scripts": {
    "start": "REACT_APP_VERSION=`git describe --tags --long --always` react-app-rewired start",
    "start:mock-user": "REACT_APP_E2E_TEST_MOCK_USER=true REACT_APP_VERSION=`git describe --tags --long --always` react-app-rewired start",
    "@comment start:no-hot-load": "start server without hot-loader, necessary for running e2e tests to prevent navigation timeout",
    "start:no-hot-load": "REACT_APP_HOT_LOADING=false REACT_APP_VERSION=`git describe --tags --long --always` react-app-rewired start",
    "@comment start:no-hot-load:mock-user": "start server with mock user, without hot-loader, necessary for running e2e tests to prevent navigation timeout",
    "start:no-hot-load:mock-user": "REACT_APP_HOT_LOADING=false REACT_APP_E2E_TEST_MOCK_USER=true REACT_APP_VERSION=`git describe --tags --long --always` react-app-rewired start",
    "build": "REACT_APP_VERSION=`git describe --tags --long --always` react-app-rewired build",
    "build:test": "env-cmd .env.test npm run build",
    "build:staging": "env-cmd .env.staging npm run build",
    "build:prod": "env-cmd .env.prod npm run build",
    "test": "REACT_APP_VERSION=`git describe --tags --long --always` react-app-rewired test",
    "test:e2e": "BROWSER=none env-cmd .env --no-override jest --config ./src/tests/e2e/jest.config.js --runInBand --bail --detectOpenHandles --verbose=true --forceExit",
    "eject": "react-scripts eject",
    "flow-typed": "flow-typed install",
    "flow": "flow",
    "eslint": "eslint \"./src/**/*.js\"",
    "snapshots:unzip": "unzip src/tests/__snapshots__",
    "snapshots:zip": "zip -r src/tests/__snapshots__.zip src/tests/__snapshots__ -x \"*.DS_Store\"",
    "snapshots:clean": "rm -fr src/tests/__snapshots__",
    "resetUser": "env-cmd .env npx babel-node src/tests/e2e/resetUser.js"
  },
